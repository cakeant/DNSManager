New User
<%= form_for @user, :url => admin_users_path, :html => {:method => :post} do |f| %>
    <div><%= f.label 'Username' %></div>
    <div><%= f.text_field :username %></div>
    <div><%= f.label 'Email' %></div>
    <div><%= f.email_field :email %></div>
    <div><%= f.label 'Administrator' %></div>
    <div><%= f.check_box :admin %></div>
    <div><%= f.label 'Password' %></div>
    <div><%= f.password_field :password %></div>
    <div><%= f.label 'Confirm Password' %></div>
    <div><%= f.password_field :password_confirmation %></div>
    <div><%= f.submit 'Create' %></div>
    <div><%= link_to 'Back', admin_users_path %></div>
<% end %>
